{
  "name": "blueprint-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "concurrently -k \"gulp watch\" \"ng serve\"",
    "watch-blueprint": "ng build --watch epsilon-blueprint",
    "build": "ng build",
    "test": "ng test",
    "stylelint": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\"",
    "stylelint-fix": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\" --fix",
    "eslint": "eslint \"src/app/**\" \"projects/epsilon-blueprint/src/lib/**\" \"projects/epsilon-blueprint/src/models/**\" --ext .ts",
    "eslint-fix": "eslint \"src/app/**\" \"projects/epsilon-blueprint/src/lib/**\" \"projects/epsilon-blueprint/src/models/**\" --ext .ts --fix",
    "e2e": "ng e2e",
    "prePublish": "ng build epsilon-blueprint && gulp copyStyles && gulp copyModels && gulp copyAssets",
    "deploy": "ng build epsilon-blueprint && gulp copyStyles && gulp copyModels && gulp copyAssets && npm run refresh-table-data 101 && ng build --prod",
    "refresh-table-data": "node scripts/refresh-table-data.js",
    "generate-data": "npm run refresh-table-data 101"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ng-select/ng-select": "^3.7.1",
    "ng-pick-datetime": "^6.0.16",
    "ngx-markdown": "^8.2.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular-devkit/build-ng-packagr": "~0.803.23",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "codelyzer": "^5.0.0",
    "concurrently": "^5.1.0",
    "eslint": "^7.4.0",
    "gulp": "^4.0.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "^5.18.0",
    "typescript": "~3.5.3"
  }
