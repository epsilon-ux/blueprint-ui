{
  "name": "blueprint-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "concurrently -k \"gulp watch\" \"ng serve\"",
    "watch-blueprint": "ng build --watch epsilon-blueprint",
    "build": "ng build",
    "test": "ng test",
    "stylelint": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\"",
    "stylelint-fix": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\" --fix",
    "eslint": "eslint \"src/app/**\" \"projects/epsilon-blueprint/src/lib/**\" \"projects/epsilon-blueprint/src/models/**\" --ext .ts",
    "eslint-fix": "eslint \"src/app/**\" \"projects/epsilon-blueprint/src/lib/**\" \"projects/epsilon-blueprint/src/models/**\" --ext .ts --fix",
    "e2e": "ng e2e",
    "prePublish": "ng build epsilon-blueprint --prod && gulp copyStyles && gulp copyModels",
    "deploy": "ng build epsilon-blueprint --prod && gulp copyStyles && gulp copyModels && npm run refresh-table-data 101 && ng build --prod",
    "refresh-table-data": "node scripts/refresh-table-data.js",
    "generate-data": "npm run refresh-table-data 101"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/cdk": "^10.2.4",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "@ng-select/ng-select": "^5.0.3",
    "d3": "^6.2.0",
    "ng-pick-datetime": "^6.0.16",
    "ngx-markdown": "^10.1.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular-devkit/build-ng-packagr": "~0.1001.2",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular/cli": "^10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@angular/language-service": "~10.1.2",
    "@types/d3": "^5.16.3",
    "@types/d3-array": "^2.3.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "eslint": "^7.4.0",
    "gulp": "^4.0.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~5.4.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~7.0.0",
    "typescript": "^3.9.7"
  }
}
